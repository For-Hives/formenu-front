		"sharp": "0.32.1",
		"tailwindcss": "3.3.2"
	},
	"devDependencies": {
		"eslint": "8.44.0",
		"husky": "8.0.3",
		"eslint-config-next": "13.4.7",
		"eslint-config-prettier": "8.8.0",
